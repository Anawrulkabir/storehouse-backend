{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/all/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
